@model SelectList

@if (Model.Count() > 0)
{
	<select asp-items="Model" name="CityId" id="CityId"  class="custom-select">
		<option disabled selected>Выберите Город</option>
	</select>
}
else
{
	<select disabled name="CityId" id="CityId"  class="custom-select">
		<option disabled selected>Выберите Город</option>
	</select>
}
